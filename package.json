    "express-fileupload": "^1.1.7-alpha.1",
    "mocha": "^7.1.1",
    "morgan": "^1.10.0",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-select-us-states": "^0.1.2",
    "sequelize": "^5.21.5",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.54",
